<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<fx:root type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <Button fx:id="buttonLeft" text="&lt;">
      <HBox.margin>
         <Insets bottom="5.0" left="5.0" right="20.0" top="5.0" />
      </HBox.margin></Button>
    <Label fx:id="label" prefHeight="22.0" prefWidth="40.0">
      <HBox.margin>
         <Insets bottom="5.0" top="5.0" />
      </HBox.margin></Label>
    <Button fx:id="buttonRight" text="&gt;">
      <HBox.margin>
         <Insets bottom="5.0" left="20.0" right="5.0" top="5.0" />
      </HBox.margin></Button>
</fx:root>
